{
  "expo": {
    "name": "Is It Vegan?",
    "slug": "isitvegan",
    "version": "4.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "net.isitvegan.free",
    "userInterfaceStyle": "automatic",
    "description": "Scan product barcodes to instantly check if they're vegan, vegetarian, or contain animal products.",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "net.isitvegan.free",
      "buildNumber": "55",
      "appleTeamId": "WP9H848GC5",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan product barcodes for vegan analysis.",
        "NSMicrophoneUsageDescription": "This app does not use the microphone.",
        "NSLocationWhenInUseUsageDescription": "This app does not actively use location services. Some included libraries may reference location APIs for enhanced functionality.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#4CAF50",
        "tabletImage": "./assets/splash-tablet.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "net.isitvegan.free",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "gradleCommand": ":app:assembleRelease",
      "buildType": "apk",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#4CAF50",
        "mdpi": "./assets/android/splash-mdpi.png",
        "hdpi": "./assets/android/splash-hdpi.png",
        "xhdpi": "./assets/android/splash-xhdpi.png",
        "xxhdpi": "./assets/android/splash-xxhdpi.png",
        "xxxhdpi": "./assets/android/splash-xxxhdpi.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#4CAF50",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Is It Vegan to access your camera to scan product barcodes."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Is It Vegan to access your photo library to select product images.",
          "cameraPermission": "Allow Is It Vegan to access your camera to take product photos."
        }
      ],
      "expo-apple-authentication",
      "./plugins/android-iap-config.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0a5b4fc-421c-4f1c-a56b-fee17a52812b"
      }
    },
    "owner": "burggraf"
  }
}